    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch main.go",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "--ado-organization=<org>",
                "--ado-project=<project>",
                "--ado-repo-id=<repo-id>",
                "--module-sync-author-name=<author-name>",
                "--module-sync-author-email=<author-email>",
                "--module-sync-source-repo-child-path=<source-repo-child-path>",
                "--use-local-identity",
                "--read-local-csv",
                "--ado-pat=<pat>"
            ]
        }
    ]
}
